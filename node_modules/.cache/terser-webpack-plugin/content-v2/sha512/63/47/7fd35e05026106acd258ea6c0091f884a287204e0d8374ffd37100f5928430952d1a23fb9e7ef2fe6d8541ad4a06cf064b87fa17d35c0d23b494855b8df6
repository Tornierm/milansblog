Click={changeTheme}> {themeChanger} </ThemeChanger>\n                    </Links>\n                    <MenuContainer showMenu={showMenu}>\n                        <Blur showMenu={showMenu} onClick={() => {setOpen(!open); displayMenu()}}/>\n                        <Menu showMenu={showMenu}>\n                            <MenuButton key={'home'} href={'/'} active={'home' === active} onClick={()=> {select('home')}}>home</MenuButton>\n                            {listItems.map((item) => \n                                <MenuButton key={item.title} href={item.url} active={item.title === active} onClick={()=> {select(item.title)}}>{item.title}</MenuButton>\n                            )}\n                            <ThemeChanger transparent={transparent} onClick={changeTheme}>\n                                <Highlight themeChanger={themeChanger}/>\n                                <Icons>\n                                    <Moon/>\n                                    <Sun/> \n                                </Icons>\n                            </ThemeChanger>\n                        </Menu>\n                    </MenuContainer>\n                </TopNav>\n            </NavContainer>\n        )\n    }\n    else {\n        return null;\n    }\n}\n\n","import React from 'react'\nimport styled from \"styled-components\";\n\nconst Info = styled.div`\nposition:relative;\npadding-bottom:4em;\ndisplay:flex;\njustify-content:center;\nalign-items:center;\nheight:var(--footer-height);\nbackground-color:var(--p-10);\nz-index:2;\nborder-top: 1px var(--p-1) solid;\n`\n\nconst Text = styled.p`\n    color:var(--p-1);\n    top:50;\n`\n\nexport default function Footer () {\n        return (\n            <Info>\n                <Text>© Milan Tornier</Text>\n            </Info>\n        )   \n}","import {useState} from 'react'\nimport styled from 'styled-components'\nimport {PrimaryButton} from '../../Styled'\nimport {postComment} from '../../../service/WPService'\n\nconst Form = styl