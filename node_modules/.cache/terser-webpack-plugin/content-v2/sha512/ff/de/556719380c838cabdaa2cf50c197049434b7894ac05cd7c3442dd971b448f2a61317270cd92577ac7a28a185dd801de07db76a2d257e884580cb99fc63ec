 = useState(false);\n    const [themeChanger, setThemeChanger] = useState(<Brightness2Icon/>);\n\n    useEffect( () => {\n        const current = localStorage.getItem('theme');\n        if(current==='dark'){\n            setThemeChanger(<WbSunnyIcon/>)\n        } else if(current==='light'){\n            setThemeChanger(<Brightness2Icon/>)\n        }\n\n        retrieveNavbar()\n        .then(res => {\n            if(res.items.type === 'undefined'){\n\n            } else {\n                const Items = res.items.map((item) =>\n                <NavItem key={item.title}>\n                    <AnchorButton key={item.title} href={item.url}>{item.title}</AnchorButton>\n                </NavItem>\n                );\n                setListItems(Items)\n                setLoaded(true);\n            }\n        });\n        return () => {\n            setLoaded(false);\n        }\n    },[])\n\n    const showLinks = () => {\n        let show = clicked;\n        setClicked(!show)\n    }\n\n    const changeTheme = () => {\n        if(props.theme==='dark'){\n          props.setTheme('light')\n          localStorage.setItem('theme', 'light');\n          setThemeChanger(<Brightness2Icon/>)\n        } else {\n          props.setTheme('dark')\n          localStorage.setItem('theme', 'dark');\n          setThemeChanger(<WbSunnyIcon/>)\n        }\n    }\n\n    if(loaded){\n        return (\n                <TopNav>\n                    <Left>\n                        <NavItem>\n                            <Logo mr href='/'>Milan Tornier</Logo>\n                        </NavItem>\n                        <NavItem>\n                            <Burger onClick={showLinks}><ReorderIcon/></Burger>\n                        </NavItem>\n                    </Left>\n                    <Right show={clicked}>\n                        {listItems}\n                        <Button onClick={changeTheme}> {themeChanger} </Button>\n                    </Right>\n                </TopNav>\n        )\n