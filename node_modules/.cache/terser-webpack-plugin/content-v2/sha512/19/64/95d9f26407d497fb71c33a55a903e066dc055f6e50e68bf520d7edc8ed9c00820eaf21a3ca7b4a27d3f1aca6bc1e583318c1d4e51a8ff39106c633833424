(--p-dark);\n    --p-1: var(--p-vdark);\n    --p-0: var(--p-black);\n\n    \n    --s-9: var(--s-vlight);\n    --s-7: var(--s-light);\n    --s-5: var(--s);\n    --s-3: var(--s-dark);\n    --s-1: var(--s-vdark);\n  `}\n  ${props => props.theme === 'dark' && css`\n  --p-10: var(--p-black);\n  --p-9: var(--p-vdark);\n  --p-7: var(--p-dark);\n  --p-5: var(--p);\n  --p-3: var(--p-light);\n  --p-1: var(--p-vlight);\n  --p-0: var(--p-white);\n  \n  --s-9: var(--s-vdark);\n  --s-7: var(--s-dark);\n  --s-5: var(--s);\n  --s-3: var(--s-light);\n  --s-1: var(--s-vlight);\n    `}\n`\n\nconst Main = styled.main`\n  min-height: calc(100vh);\n  background-color:var(--p-10);\n`\n\nfunction App() {\n\n  const [theme, setTheme] = useState()\n\n  useEffect(() =>{\n    const current = localStorage.getItem('theme');\n    if(current){\n      setTheme(current)\n    } else {\n      setTheme('light')\n    }\n  },[])\n\n  return (\n    <Site theme={theme}>\n      <Navbar theme={theme} setTheme={x => setTheme(x)}/>\n      <Main>\n        <Router>\n          <ScrollToTop/>\n          <Switch>\n            <Route exact path=\"/\">\n              <Sidebar/>\n              <Welcome />\n              <PostSlider category=\"photo\"/>\n              <InstaPreview/>\n            </Route>\n            <Route exact path=\"/blog\">\n              <Posts category=\"blog\"/>\n            </Route>\n            <Route exact path=\"/photo\">\n              <Posts category=\"photo\"/>\n            </Route>\n            <Route path=\"/post/:id\" component={PostPage}/>\n          </Switch>\n        </Router>\n      </Main>\n      <Footer/>\n    </Site>\n  );\n}\n\nexport default App;\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport App from './App';\nimport '../node_modules/bootstrap/dist/css/bootstrap.min.css';\nimport '../node_modules/bootstrap/dist/js/bootstrap';\n\nimport './App.css'\n\nReactDOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById('root')\n);"]}}