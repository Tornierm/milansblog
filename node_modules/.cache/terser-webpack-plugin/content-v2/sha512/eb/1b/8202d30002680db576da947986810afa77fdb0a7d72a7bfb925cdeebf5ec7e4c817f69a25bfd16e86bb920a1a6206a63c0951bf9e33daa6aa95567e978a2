up() {\n            setIsLoaded(false);\n        }\n    },[])\n\n\n        if(isLoaded){\n            return (\n                <GridWrapper id=\"ig\">\n                    <SectionTitle>Instagram</SectionTitle>\n                    <ImageGrid>\n                        { posts.map((post, index) => (\n                            <ImageContainer \n                                key={'ig'+post.id}\n                                clicked={selected === post.id} \n                                image={post.media_url}\n                                order={(1+index)*2}\n                                onClick={ () => {\n                                    if(selected !== post.id){\n                                        setSelected(post.id)\n                                    }\n                                    else{\n                                        setSelected(null)\n                                    }\n                                }}\n                            />\n                        ))}\n                    </ImageGrid>\n                    <PrimaryButton onClick={() => {window.location.href=\"https://www.instagram.com/milan.tornier\"}}>Visit</PrimaryButton>\n                </GridWrapper>\n            )\n        }\n        else{\n            return (\n                <Wrapper id=\"ig\">\n                    <Spinner/>\n                </Wrapper>\n            )\n        }\n}\n\n\n","import React from 'react'\nimport Sidebar from '../components/Nav/Sidebar'\nimport Welcome from '../components/Sections/Welcome'\nimport PostSlider from '../components/Sections/PostSlider/PostSlider'\nimport InstaPreview from '../components/Sections/InstaPreview'\n\nimport styled from 'styled-components';\n\nconst Page = styled.div`\n    @media (min-width: 48em) {\n        max-width:80em;\n    }   \n    @media (min-width: 64em) {\n        margin-left:auto;\n        margin-right:auto;\n    }    \n`\nconst Sections = styled.div`\n    margin:0;\n    @media (min-width