Effect} from 'react'\nimport styled from 'styled-components'\nimport { retrieveFeaturedMedia, retrievePageByName } from '../../service/WPService';\nimport {Spinner, Loading, Wrapper, Title} from '../Styled'\nimport Instagram from '@material-ui/icons/Instagram';\nimport LinkedIn from '@material-ui/icons/LinkedIn';\n\n\nconst StyledWrapper = styled(Wrapper)`\n    padding-top:0;\n    margin:0;\n    @media (min-width: 32em) {\n        margin:0 var(--margin);\n        flex-direction:column;\n        justify-content:center;\n        display:flex;\n    }\n`\n\nconst Upper = styled.div`\n    display:flex;\n    flex-direction:column;\n    height:auto;\n    margin-bottom:var(--margin);\n    @media (min-width: 32em) {\n        flex-direction:row;\n    }\n`\n\nconst Image = styled.img`\n    object-fit: cover;\n    display:block;\n    width:100%;\n    height:100%;\n    margin-bottom:var(--margin);\n    @media (min-width: 32em) {\n        width:50%;\n        margin-bottom:0;\n        order:3;\n    }\n`\n\nconst Info = styled.div`\n    display:flex;  \n    flex-direction:column;\n    justify-content:space-around;\n    height:auto;\n    padding:var(--margin);\n    @media (min-width: 32em) {\n        width:50%;\n        margin-right:var(--margin);\n        padding:0;\n    }\n`\nconst Social = styled.div`\n    \n    @media (min-width: 32em) {\n        margin:calc(var(--margin)) 0;\n        order:3;\n    }\n`\n\nconst SocialList = styled.ul`\n    display:flex;\n    justify-content:flex-end;\n    margin:0;\n    padding:0;\n    width:100%;\n    justify-content:flex-start;\n    @media (min-width: 32em) {\n        align-items:flex-start;\n        justify-content:flex-start;\n\n    }\n`\n\nconst SocialItem = styled.li`\n    list-style-type: none;\n    display:flex;\n    align-items:center;\n    justify-content:center;\n    padding:calc(var(--margin)/2) calc(2*var(--margin)) calc(var(--margin)/2) 0;\n    @media (min-width: 32em) {\n        padding:calc(var(--margin)/2) 0 0 0;\n        margi